{
  "name": "gatsby-starter-typescript-ioc-mobx",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Maxim Karpov <makame@mail.ru>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "jest": "jest",
    "test": "run-s lint jest"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(jsx?|tsx?)$": "<rootDir>/scripts/jest-preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/file-mock.js"
    },
    "testURL": "http://localhost",
    "testRegex": "\\/src\\/.+\\.(test|spec)\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "babel-preset-gatsby": "^0.2.27",
    "classnames": "^2.2.6",
    "debounce": "^1.2.0",
    "gatsby": "^2.18.23",
    "gatsby-plugin-layout": "^1.1.21",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-styled-jsx": "^3.1.18",
    "gatsby-plugin-typescript": "^2.1.26",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.5",
    "mobx-react-lite": "^1.5.2",
    "paper": "^0.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-ioc": "^1.0.0",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.12.0",
    "styled-jsx": "^3.2.4",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/debounce": "^1.2.0",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.7",
    "@types/paper": "^0.12.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-blvd": "^1.0.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  }
}
